<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN" "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">

<urlrewrite>   
    <rule>
        <from>^/product/(\d+)$</from>
        <to last="true">/product.seam?id=$1</to>
    </rule>

    <rule>
        <from>^/browse/(\w+)$</from>
        <to last="true">/browse.seam?name=$1</to>
    </rule>


    <rule>
        <from>^/browse/(\d+)$</from>
        <to last="true">/browse.seam?cat=$1</to>
    </rule>
    
    <rule>
        <from>^/([/a-zA-Z_]*)$</from>
        <to last="true">/$1.seam</to>
    </rule>

    
    <outbound-rule>
        <from>/product.seam\?id=(\d+)&amp;(.*)</from>
        <to>/product/$1?$2</to>
    </outbound-rule>
    
    <outbound-rule>
        <from>/product.seam\?id=(\d+)</from>
        <to>/product/$1</to>
    </outbound-rule>

    <outbound-rule>
        <from>/product.seam\?(.*)&amp;id=(\d+)</from>
        <to>/product/$2?$1</to>
    </outbound-rule>

    <outbound-rule>
        <from>/browse.seam\?name=(\w+)&amp;(.*)</from>
        <to>/browse/$1?$2</to>
    </outbound-rule>



    <outbound-rule>
        <from>/browse.seam\?name=(\w+)</from>
        <to>/browse/$1</to>
    </outbound-rule>



    <outbound-rule>
        <from>/browse.seam\?cat=(\d+)&amp;(.*)</from>
        <to>/browse/$1?$2</to>
    </outbound-rule>

    <outbound-rule>
        <from>/browse.seam\?cat=(\d+)</from>
        <to>/browse/$1</to>
    </outbound-rule>




    <outbound-rule>
        <from>/([a-z]+)\.seam</from>
        <to>/$1</to>
    </outbound-rule>
</urlrewrite>